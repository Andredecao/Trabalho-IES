#include <stdio.h>
                                            // So consegui usando if 
int main() {

float preco;

    printf("Preco do produto: ");
    
    scanf("%f", &preco);

    if(preco < 100) {

        preco = preco * 1.1;

    }else{

        preco = preco * 1.2;

    }

    printf("Preco inflacionado: %.2f\n", preco);

    return 0;

}
